(type Name)
(representation Unstructured Name)
(constructor Name Unstructured ((String:Native x)) x)
(representation Structured Name)
(constructor Name Structured ((String:Native x) (String:Native y)) (cons x y))
(println ((extended-lambda (x) ((Name:Unstructured) "unstructured") ((Name:Structured) "structured")) (construct Name Unstructured "Jan Novak")))
(println ((extended-lambda (x) ((Name:Structured) "structured")) (construct Name Structured "Jan" "Novak")))
(conversion Name:Structured Name:Unstructured ((Name:Structured x)) (construct Name Unstructured (concat (car (deconstruct x)) (cdr (deconstruct x)))))
(println ((lambda ((Name:Unstructured x)) x) (construct Name Structured "Jan" "Novak")))

